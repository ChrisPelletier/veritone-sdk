{
	"rules": {
		"no-console": 0
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": true,
		"mocha": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"impliedStrict": true,
			"experimentalObjectRestSpread": true
		}
	},
	"extends": ["eslint:recommended", "prettier"]
}
